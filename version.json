{
  "$ref": "base.json",
  "title" :  "Version",
  "description" :  "Version message",
  "type" :  "object",
  "properties" : {
    "type" : {
      "description" : "Type of RMSP message",
      "type" : "string",
      "pattern" : "^Version$"
    },
    "RSMP" : {
      "description" : "Supported RSMP versions",
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "vers" : {
            "type" : "string",
            "pattern" : "^[0-9]{1,2}\\.[0-9]{1,2}(\\.[0-9]{1,2})?$"
          }
        }
      },
      "minItems" : 1,
      "uniqueItems" : true,
      "additionalProperties" : false
    },
    "SXL" : {
      "description" : "SXL version",
      "type" : "string",
      "pattern" : "^[0-9]{1,2}\\.[0-9]{1,2}(\\.[0-9]{1,2})?$"
    }
  },
  "required" : [ "type", "RSMP", "SXL" ],
  "additionalProperties" : false
}