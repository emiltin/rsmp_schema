{
  "title" :  "CommandRequest",
  "description" :  "Command request",
  "type" :  "object",
  "properties" : {
    "cId" : {
      "description" : "Compoment id",
      "type" : "string"
    },
    "arg" : {
      "description" : "Command arguments",
      "type" : "array",
      "minItems": 1,
      "items" : {
        "type" : "object",
        "properties": {
          "cCI" : {
            "description" : "Command code id",
            "type" : "string",
            "pattern" : "^M[0-9]{4}$"
          },
          "n" : {
            "description" : "Unique reference of the value",
            "type" : "string"
          },
          "cO" : {
            "description" : "Command",
            "type" : "string"
          },
          "v" : {
            "description" : "Value",
            "type" : "string"
          }
        },
        "required" : [ "cCI", "n", "cO", "v" ]
      }
    }
  },
  "required" : ["cId", "arg"]
}